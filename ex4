#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
#include <math.h>
#include <float.h>

int main(void)
{
    double podwojnaPrecyzja;
    
    system("clear");
//Praca z double
printf("Wprowadz wartosc typu double: ");
scanf("%lf", &podwojnaPrecyzja);
printf("Liczba typu: double\t\t%.10lf zajmuje %ld bajt(y)(ow)") podwojnaPrecyzja, sizeof(podwojnaPrecyzja));
printf("\nLiczba typu: double\t\t%.10le zajmuje %2ld bajt(y)(ow)"
    podwojnaPrecyzja, sizeof(podwojnaPrecyzja));
    return 0;
}
